<!-- Mainbar head starts -->
<div class="main-head">
  <div class="container">
    <div class="row">
      <div class="col-md-5 col-sm-4 col-xs-6">
        <% if @map.user == current_user %>
          <h4><i class="fa fa-map lblue"></i> Editing your map: <%= @map.name %></h4>
        <% end %>
      </div>
    </div>
  </div>
</div>
<!-- Mainbar head ends -->

<%= render partial: "form", locals: {map: @map} %>